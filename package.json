{
  "name": "box-di",
  "description": "A wrapper around kontainer-di module that allows decorators to inject dependencies. I wanted to encourage the use of the factory pattern when using DI so that your classes can be reused and tested without framework dependencies ie. es7 decorators etc",
  "version": "0.0.5",
  "author": "Josh Stuart <crimsonronin@gmail.com>",
  "keywords": [
    "di",
    "factory",
    "dependency injection",
    "ioc",
    "inversion of control",
    "solid"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crimsonronin/box-di.git"
  },
  "main": "./dist/index.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "stage-1"
    ],
    "plugins": [
      "babel-plugin-transform-decorators-legacy"
    ]
  },
  "scripts": {
    "build-server": "babel ./index.js --out-dir ./dist && babel ./lib --out-dir ./dist/lib && babel ./example --out-dir ./dist/example",
    "build": "npm run clean && npm run build-server",
    "clean": "rm -rf ./dist && mkdir ./dist",
    "prepublish": "npm run build",
    "lint": "./node_modules/.bin/eslint ./lib ./test",
    "test": "./node_modules/.bin/babel-node ./example/example.js"
  },
  "dependencies": {
    "kontainer-di": "^0.9.9",
    "lodash": "^4.11.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "5.0.0-beta9",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-etaskr": "^1.0.2"
  }
}
